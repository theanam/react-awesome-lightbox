{
    "name": "react-awesome-lightbox",
    "version": "1.8.1",
    "description": "lightbox image viewer for react with zoom, rotate. Single or multi image mode and touch support",
    "keywords": [
        "react",
        "image viewer",
        "lightbox",
        "image zoom",
        "image rotate",
        "image gallery",
        "react lightbox",
        "responsive lightbox",
        "reactjs"
    ],
    "main": "build/index.js",
    "peerDependencies": {
        "react": "^16||^17||^18"
    },
    "scripts": {
        "start": "webpack --watch",
        "build": "webpack"
    },
    "author": {
        "name": "Anam Ahmed",
        "email": "anam.ahmed.a@gmail.com"
    },
    "devDependencies": {
        "@babel/core": "^7.10.4",
        "babel-loader": "8.1.0",
        "file-loader": "4.3.0",
        "url-loader": "2.3.0",
        "webpack": "4.42.0",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-react": "^7.10.4",
        "webpack-cli": "^3.3.12"
    },
    "babel": {
        "presets": [
            "@babel/preset-react",
            "@babel/preset-env"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git@github.com:theanam/react-awesome-lightbox.git"
    },
    "dependencies": {}
